<?xml version="1.0" encoding="utf-8"?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="ASRSummary">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="UcrCodeDictionary">
          <xs:complexType>
            <xs:sequence>
              <xs:element maxOccurs="unbounded" name="UCRDescription">
                <xs:complexType>
                  <xs:simpleContent>
                    <xs:extension base="xs:string">
                      <xs:attribute name="value" type="xs:string" use="required" />
                    </xs:extension>
                  </xs:simpleContent>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element maxOccurs="unbounded" name="UCR">
          <xs:complexType>
            <xs:sequence>
              <xs:element maxOccurs="unbounded" name="Age">
                <xs:complexType>
                  <xs:sequence>
                    <xs:choice maxOccurs="unbounded">
                      <xs:element name="M" type="xs:int" />
                      <xs:element name="F" type="xs:int" />
                    </xs:choice>
                  </xs:sequence>
                  <xs:attribute name="value" type="xs:string" use="required" />
                </xs:complexType>
              </xs:element>
              <xs:element name="Races">
                <xs:complexType>
                  <xs:sequence>
                    <xs:choice maxOccurs="unbounded">
                      <xs:element name="White" type="xs:int" />
                      <xs:element name="Black" type="xs:int" />
                    </xs:choice>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="Ethnicities">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element minOccurs="0" name="Non-Hispanic" type="xs:int" />
                    <xs:element minOccurs="0" name="Hispanic" type="xs:int" />
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
            <xs:attribute name="value" type="xs:string" use="required" />
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>